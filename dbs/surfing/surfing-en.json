            "item_title": [
                {
                    "type": "heading2",
                    "text": "LA ISLA",
                    "spans": []
                }
            ],
            "item_description": [
                {
                    "type": "paragraph",
                    "text": "La Isla is not only worth going to for its long peeling lefts that blows your mind but also the adventure of going through the estuary to visit this remote island is a rich experience itself. The left is so long that your arms will be sore the next day from all the paddling!",
                    "spans": []
                }
            ]
        },
